#include <unistd.h>
#include "main.h"
/**
* times_table - Prints the 9 times table
* 
* Description: This function prints the 9 times table, starting with 0.
*
* Return: Always 0 (Success)
*/
void times_table(void)
{
	int i, pr;

	for (i = 0; i < 10; i++)
	{
		pr = 9 * i;
		_putchar(pr + '0');
		if (i < 9)
		{
			_putchar(',');
			_putchar(' ');
		}
	}
}
